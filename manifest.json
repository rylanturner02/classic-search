{
    "manifest_version": 3,
    "name": "Classic Web Search",
    "version": "1.0",
    "description": "Automatically filters by Web results only.",
    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
      "*://*.google.com/search*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    }
  }
  